        <Button Content="Добавить" HorizontalAlignment="Left" Margin="296,370,0,0" VerticalAlignment="Top" Height="34" Width="134" Grid.Column="1" x:Name="BAddOffice" Click="BAddOffice_Click"/>
        <Button Content="Выдать" HorizontalAlignment="Left" Margin="481,370,0,0" VerticalAlignment="Top" Height="34" Width="134" Grid.Column="1" x:Name="BGetList" Click="BGetList_Click"/>
        <Label Grid.Column="1" Content="" HorizontalAlignment="Left" Margin="449,52,0,0" VerticalAlignment="Top" Height="276" Width="284" x:Name="ListRooms"/>
        <Button Grid.Column="1" Content="Очистить" HorizontalAlignment="Left" Margin="651,370,0,0" VerticalAlignment="Top" RenderTransformOrigin="0.656,0.052" Height="34" Width="72" Click="Button_Click"/>

