        <Rectangle HorizontalAlignment="Left" Height="344" Margin="283,10,0,0" Stroke="Black" VerticalAlignment="Top" Width="161" Grid.Column="1"/>
        <Rectangle HorizontalAlignment="Left" Height="344" Margin="283,10,0,0" Stroke="Black" VerticalAlignment="Top" Width="161" Grid.Column="1" Fill="#FF10E8E3"/>
