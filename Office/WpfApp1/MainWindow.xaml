        <Label Content="Ширина" HorizontalAlignment="Left" Margin="12,76,0,0" VerticalAlignment="Top" FontSize="13" Grid.ColumnSpan="2"/>
        <Label Content="Длина" HorizontalAlignment="Left" Margin="12,44,0,0" VerticalAlignment="Top" FontSize="13" Grid.ColumnSpan="2"/>
        <TextBox HorizontalAlignment="Left" Margin="41,44,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="61" Height="27" Grid.Column="1" x:Name="TBLengthR"/>
        <TextBox HorizontalAlignment="Left" Margin="41,76,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="61" Height="27" Grid.Column="1" x:Name="TBWidthR"/>
        <Label Content="Ширина" HorizontalAlignment="Left" Margin="129,76,0,0" VerticalAlignment="Top" FontSize="13" Grid.Column="1"/>
        <Label Content="Длина" HorizontalAlignment="Left" Margin="129,44,0,0" VerticalAlignment="Top" FontSize="13" Grid.Column="1"/>
        <TextBox HorizontalAlignment="Left" Margin="217,44,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="61" Height="27" Grid.Column="1" x:Name="TBLengthL"/>
        <TextBox HorizontalAlignment="Left" Margin="217,76,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="61" Height="27" Grid.Column="1" x:Name="TBWidthL"/>
        <Label Content="Ширина" HorizontalAlignment="Left" Margin="284,76,0,0" VerticalAlignment="Top" FontSize="13" Grid.Column="1"/>
        <Label Content="Длина" HorizontalAlignment="Left" Margin="284,44,0,0" VerticalAlignment="Top" FontSize="13" Grid.Column="1"/>
        <TextBox HorizontalAlignment="Left" Margin="378,44,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="60" Height="27" Grid.Column="1" x:Name="TBLengthO"/>
        <TextBox HorizontalAlignment="Left" Margin="378,76,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="60" Height="27" Grid.Column="1" x:Name="TBWidthO"/>
